package com.mindfire.weatherservice.dto;

import com.fasterxml.jackson.annotation.JsonAlias;
import com.fasterxml.jackson.annotation.JsonProperty;

import lombok.Data;

@Data
public class Current
{
    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
	private int lastUpdatedEpoch;
    
    @JsonProperty("lastUpdated")
    @JsonAlias("last_updated")
    private String lastUpdated;
    
    @JsonProperty("tempC")
    @JsonAlias("temp_c")
    private int tempC;
    
    @JsonProperty("tempF")
    @JsonAlias("temp_f")
    private double tempF;
    
    @JsonProperty("isDay")
    @JsonAlias("is_day")
    private int isDay;

    private Condition condition;
    
    @JsonProperty("windMph")
    @JsonAlias("wind_mph")
    private double windMph;

    @JsonProperty("windKph")
    @JsonAlias("wind_kph")
    private double windKph;
    
    @JsonProperty("windDegree")
    @JsonAlias("wind_degree")
    private int windDegree;
    
    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
    private String wind_dir;
    
    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
    private int pressure_mb;
    
    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
    private double pressure_in;
    
    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
    private int precip_mm;
    
    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
    private int precip_in;

    private int humidity;

    private int cloud;
    
    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
    private double feelslike_c;
    
    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
    private double feelslike_f;
    
    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
    private int vis_km;
    
    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
    private int vis_miles;

    private int uv;

    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
    private double gust_mph;
    
    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
    private double gust_kph;
    
    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
    private AirQuality air_quality;

    public void setLast_updated_epoch(int last_updated_epoch){
        this.last_updated_epoch = last_updated_epoch;
    }
    public int getLast_updated_epoch(){
        return this.last_updated_epoch;
    }
    public void setLast_updated(String last_updated){
        this.last_updated = last_updated;
    }
    public String getLast_updated(){
        return this.last_updated;
    }
    public void setTemp_c(int temp_c){
        this.temp_c = temp_c;
    }
    public int getTemp_c(){
        return this.temp_c;
    }
    public void setTemp_f(double temp_f){
        this.temp_f = temp_f;
    }
    public double getTemp_f(){
        return this.temp_f;
    }
    public void setIs_day(int is_day){
        this.is_day = is_day;
    }
    public int getIs_day(){
        return this.is_day;
    }
    public void setCondition(Condition condition){
        this.condition = condition;
    }
    public Condition getCondition(){
        return this.condition;
    }
    public void setWind_mph(double wind_mph){
        this.wind_mph = wind_mph;
    }
    public double getWind_mph(){
        return this.wind_mph;
    }
    public void setWind_kph(double wind_kph){
        this.wind_kph = wind_kph;
    }
    public double getWind_kph(){
        return this.wind_kph;
    }
    public void setWind_degree(int wind_degree){
        this.wind_degree = wind_degree;
    }
    public int getWind_degree(){
        return this.wind_degree;
    }
    public void setWind_dir(String wind_dir){
        this.wind_dir = wind_dir;
    }
    public String getWind_dir(){
        return this.wind_dir;
    }
    public void setPressure_mb(int pressure_mb){
        this.pressure_mb = pressure_mb;
    }
    public int getPressure_mb(){
        return this.pressure_mb;
    }
    public void setPressure_in(double pressure_in){
        this.pressure_in = pressure_in;
    }
    public double getPressure_in(){
        return this.pressure_in;
    }
    public void setPrecip_mm(int precip_mm){
        this.precip_mm = precip_mm;
    }
    public int getPrecip_mm(){
        return this.precip_mm;
    }
    public void setPrecip_in(int precip_in){
        this.precip_in = precip_in;
    }
    public int getPrecip_in(){
        return this.precip_in;
    }
    public void setHumidity(int humidity){
        this.humidity = humidity;
    }
    public int getHumidity(){
        return this.humidity;
    }
    public void setCloud(int cloud){
        this.cloud = cloud;
    }
    public int getCloud(){
        return this.cloud;
    }
    public void setFeelslike_c(double feelslike_c){
        this.feelslike_c = feelslike_c;
    }
    public double getFeelslike_c(){
        return this.feelslike_c;
    }
    public void setFeelslike_f(double feelslike_f){
        this.feelslike_f = feelslike_f;
    }
    public double getFeelslike_f(){
        return this.feelslike_f;
    }
    public void setVis_km(int vis_km){
        this.vis_km = vis_km;
    }
    public int getVis_km(){
        return this.vis_km;
    }
    public void setVis_miles(int vis_miles){
        this.vis_miles = vis_miles;
    }
    public int getVis_miles(){
        return this.vis_miles;
    }
    public void setUv(int uv){
        this.uv = uv;
    }
    public int getUv(){
        return this.uv;
    }
    public void setGust_mph(double gust_mph){
        this.gust_mph = gust_mph;
    }
    public double getGust_mph(){
        return this.gust_mph;
    }
    public void setGust_kph(double gust_kph){
        this.gust_kph = gust_kph;
    }
    public double getGust_kph(){
        return this.gust_kph;
    }
    public void setAir_quality(AirQuality air_quality){
        this.air_quality = air_quality;
    }
    public AirQuality getAir_quality(){
        return this.air_quality;
    }
}