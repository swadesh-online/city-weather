package com.mindfire.weatherservice.dto;

import com.fasterxml.jackson.annotation.JsonAlias;
import com.fasterxml.jackson.annotation.JsonProperty;

import lombok.Data;

@Data
public class Current
{
    @JsonProperty("lastUpdatedEpoch")
    @JsonAlias("last_updated_epoch")
	private int lastUpdatedEpoch;
    
    @JsonProperty("lastUpdated")
    @JsonAlias("last_updated")
    private String lastUpdated;
    
    @JsonProperty("tempC")
    @JsonAlias("temp_c")
    private int tempC;
    
    @JsonProperty("tempF")
    @JsonAlias("temp_f")
    private double tempF;
    
    @JsonProperty("isDay")
    @JsonAlias("is_day")
    private int isDay;

    private Condition condition;
    
    @JsonProperty("windMph")
    @JsonAlias("wind_mph")
    private double windMph;

    @JsonProperty("windKph")
    @JsonAlias("wind_kph")
    private double windKph;
    
    @JsonProperty("windDegree")
    @JsonAlias("wind_degree")
    private int windDegree;
    
    @JsonProperty("windDir")
    @JsonAlias("wind_dir")
    private String windDir;
    
    @JsonProperty("pressureMb")
    @JsonAlias("pressure_mb")
    private int pressureMb;
    
    @JsonProperty("pressureIn")
    @JsonAlias("pressure_in")
    private double pressureIn;
    
    @JsonProperty("precipMm")
    @JsonAlias("precip_mm")
    private int precipMm;
    
    @JsonProperty("precipIn")
    @JsonAlias("precip_in")
    private int precip_in;

    private int humidity;

    private int cloud;
    
    @JsonProperty("feelslikeC")
    @JsonAlias("feelslike_c")
    private double feelslikeC;
    
    @JsonProperty("feelslikeF")
    @JsonAlias("feelslike_f")
    private double feelslikeF;
    
    @JsonProperty("visKm")
    @JsonAlias("vis_km")
    private int visKm;
    
    @JsonProperty("visMiles")
    @JsonAlias("vis_miles")
    private int visMiles;

    private int uv;

    @JsonProperty("gustMph")
    @JsonAlias("gust_mph")
    private double gustMph;
    
    @JsonProperty("gustKph")
    @JsonAlias("gust_kph")
    private double gustKph;
    
    @JsonProperty("airQuality")
    @JsonAlias("air_quality")
    private AirQuality airQuality;

   
}