package com.mindfire.weatherservice.dto;

import com.fasterxml.jackson.annotation.JsonAlias;
import com.fasterxml.jackson.annotation.JsonProperty;

import lombok.Data;

@Data
public class Day
{	
	
	@JsonProperty("maxtempC")
	@JsonAlias("maxtemp_c")
    private double maxtempC;
	
	@JsonProperty("maxtempF")
	@JsonAlias("maxtemp_f")
    private double maxtempF;
	
	@JsonProperty("mintempC")
	@JsonAlias("mintemp_c")
    private double mintempC;
	
	@JsonProperty("mintempF")
	@JsonAlias("mintemp_f")
    private double mintempF;
	
	@JsonProperty("avgtempC")
	@JsonAlias("avgtemp_c")
    private int avgtempC;
	
	@JsonProperty("avgtempF")
	@JsonAlias("avgtemp_f")
    private double avgtempF;
	
	@JsonProperty("maxwindMph")
	@JsonAlias("maxwind_mph")
    private double maxwindMph;
	
	@JsonProperty("maxwindKph")
	@JsonAlias("maxwind_kph")
    private double maxwindKph;
	
	@JsonProperty("totalprecipMm")
	@JsonAlias("totalprecip_mm")
    private int totalprecipMm;
	
	@JsonProperty("totalprecipIn")
	@JsonAlias("totalprecip_in")
    private int totalprecipIn;
	
	@JsonProperty("avgvisKm")
	@JsonAlias("avgvis_km")
    private int avgvisKm;

    private int avgvis_miles;

    private int avghumidity;

    private int daily_will_it_rain;

    private int daily_chance_of_rain;

    private int daily_will_it_snow;

    private int daily_chance_of_snow;

    private Condition condition;

    private int uv;

}