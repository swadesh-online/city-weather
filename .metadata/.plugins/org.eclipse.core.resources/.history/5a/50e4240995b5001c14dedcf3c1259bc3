package com.mindfire.cityservice.dto;

public class CityWeatherReport {
	private String city;
	private String condition;
	private String temperature;
	private String humidity;
	
	public CityWeatherReport(String city, String condition, int temperature, int humidity) {
		super();
		this.city = city;
		this.condition = condition;
		this.temperature = Integer.toString(temperature);
		this.humidity = Integer.toString(humidity);
	}

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}

	public String getcondition() {
		return condition;
	}

	public void setcondition(String condition) {
		this.condition = condition;
	}

	public String getTemperature() {
		return temperature;
	}

	public void setTemperature(String temperature) {
		this.temperature = temperature;
	}

	public String getHumidity() {
		return humidity;
	}

	public void setHumidity(String humidity) {
		this.humidity = humidity;
	}
	
}
